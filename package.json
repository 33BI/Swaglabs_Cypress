{
  "name": "swaglabs-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rimraf cypress/reports && node -e \"require('fs').mkdirSync('cypress/reports/mocha',{recursive:true}); require('fs').mkdirSync('cypress/reports/mochareports',{recursive:true})\"",
    "pretest": "npm run clean:reports",
    "cypress:run": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json -o cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run cypress:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serenity-dojo/swaglabs-cypress.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serenity-dojo/swaglabs-cypress/issues"
  },
  "homepage": "https://github.com/serenity-dojo/swaglabs-cypress#readme",
  "devDependencies": {
    "cypress": "^15.6.0",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.1.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.3.2",
    "rimraf": "^6.1.0"
  }
}
